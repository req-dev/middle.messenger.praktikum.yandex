.submenu-button {
    align-items: center;
    background: transparent;
    cursor: pointer;
    display: flex;
    height: 38px;
    margin: 0;
    text-align: start;
    width: 100%;
}

.submenu-button-icon {
    padding: 0 15px;
    width: 22px;
    height: 22px;
}

.submenu-button-text {
    font-family: Inter, sans-serif;
    font-size: 12px;
    font-weight: 500;
    line-height: 100%;
    user-select: none;
}
